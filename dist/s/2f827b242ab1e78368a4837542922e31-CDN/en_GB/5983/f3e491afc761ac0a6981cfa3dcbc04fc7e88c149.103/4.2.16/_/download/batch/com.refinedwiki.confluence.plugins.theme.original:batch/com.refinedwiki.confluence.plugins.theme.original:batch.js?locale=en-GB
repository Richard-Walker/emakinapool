    for (var roleIndex438 = 0; roleIndex438 < roleListLen438; roleIndex438++) {
      var roleData438 = roleList438[roleIndex438];
      output.append(soy.$$escapeHtml(roleData438), (roleIndex438 != opt_data.userCategory.roles.length - 1) ? ', ' : '');
    }
  }
